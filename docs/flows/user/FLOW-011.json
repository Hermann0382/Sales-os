    },
    {
      "id": "STEP-008",
      "sequence": 8,
      "action": "Alignment check on work reduction belief",
      "actor": "user",
      "inputs": [],
      "outputs": ["alignment_question_asked"],
      "conditions": [],
      "next_steps": ["STEP-009"]
    },
    {
      "id": "STEP-009",
      "sequence": 9,
      "action": "Capture alignment response",
      "actor": "user",
      "inputs": ["alignment_response"],
      "outputs": ["alignment_captured"],
      "conditions": [],
