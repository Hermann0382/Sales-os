        {
          "condition": "response == new_project_resistance",
          "outcome": "explore_project_fear"
        }
      ],
      "next_steps": ["STEP-005"]
    },
    {
      "id": "STEP-005",
      "sequence": 5,
      "action": "Identify core fear",
