    },
    {
      "id": "STEP-010",
      "sequence": 10,
      "action": "Agent adds diagnostic notes",
      "actor": "user",
      "inputs": ["objection_notes"],
      "outputs": ["notes_saved"],
      "conditions": [],
      "next_steps": ["STEP-011"]
    },
    {
      "id": "STEP-011",
      "sequence": 11,
      "action": "Select forced outcome",
      "actor": "user",
      "inputs": ["objection_outcome"],
