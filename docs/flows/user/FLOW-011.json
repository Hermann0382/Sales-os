    {
      "id": "STEP-012",
      "sequence": 12,
      "action": "System records ObjectionResponse and routes accordingly",
      "actor": "system",
      "inputs": ["call_session_id", "objection_id", "milestone_id", "outcome", "notes"],
      "outputs": ["objection_response_saved", "routing_determined"],
      "conditions": [],
      "next_steps": []
    }
